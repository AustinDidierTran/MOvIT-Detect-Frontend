{
  "name": "react-es6-webpack-boilerplate",
  "version": "5.0.0",
  "description": "Boilerplate for kick starting a React Project with ES6 (Babel) and Hot reloader using Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "npm i && node server.js",
    "build": "npm i && cross-env BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js",
    "lint": "eslint --ext .js *.js src/",
    "test": "jest tests/* --coverage",
    "prod": "node server.production.js",
    "pi": "NODE_ENV=pi node server.js",
    "local": "NODE_ENV=local node server.js",
    "precommit": "npm run lint",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vasanthk/react-es6-webpack-boilerplate.git"
  },
  "keywords": [
    "react",
    "es6",
    "babel",
    "webpack"
  ],
  "author": "Vasanth Krishnamoorthy (http://www.vasanthk.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasanthk/react-es6-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/vasanthk/react-es6-webpack-boilerplate",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.20.0",
    "chart.js": "^2.7.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "i18n-react": "^0.6.3",
    "json-loader": "^0.5.7",
    "primereact": "^1.4.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-easy-chart": "^1.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "url-loader": "^1.0.1",
    "victory": "^0.25.7",
    "yaml-loader": "^0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^22.2.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-compat": "^0.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.2.1",
    "react-test-renderer": "^16.2.0",
    "react-transform-hmr": "^1.0.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
